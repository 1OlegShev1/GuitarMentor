{
  "name": "guitar-mentor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0 -p 3001",
    "build": "next build",
    "start": "next start -H 0.0.0.0 -p 3001",
    "lint": "next lint",
    "find-ip": "node scripts/find-local-ip.js",
    "dev:network": "node scripts/find-local-ip.js && next dev -H 0.0.0.0 -p 3001",
    "dev:https": "next dev -p 3001",
    "proxy:https": "local-ssl-proxy --source 3002 --target 3001",
    "dev:secure": "concurrently \"npm run dev:https\" \"npm run proxy:https\"",
    "build:css": "npx tailwindcss -i ./src/styles/globals.css -o ./src/styles/output.css --minify",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "aubiojs": "^0.2.1",
    "next": "^15.2.3",
    "next-themes": "^0.4.6",
    "pitchy": "^4.0.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "tonal": "^6.4.1",
    "tone": "^15.0.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.11",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "eslint-config-next": "^15.2.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "local-ssl-proxy": "^2.0.5",
    "postcss": "^8.4.35",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.3.0",
    "typescript": "^5.3.3"
  }
}
